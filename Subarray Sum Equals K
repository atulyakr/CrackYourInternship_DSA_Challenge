class Solution {
public:
    int subarraySum(vector<int>& nums, int k) {
        int count=0;
        unordered_map<int,int>mp;
        mp.insert({0,1});

        int culsum=0;
        for(int i=0;i<nums.size();i++)
        {
            culsum+=nums[i];
            auto itr=mp.find(culsum-k);
            if(itr!=mp.end())
            {
                count+=itr->second;
            }
            mp[culsum]++;
        }
        return count;
    }
};
